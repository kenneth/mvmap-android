<?xml version="1.0" encoding="utf-8"?><resources>
    <!--Start attrs definition block-->
    <attr name="demoListRowViewStyle" format="reference"/>
    <attr name="disableMusicIcon" format="reference"/>
    <attr name="menuBackground" format="reference|color"/>
    <attr name="selectionHandler" format="color|reference"/>
    <attr name="selectionHandlerVisiblity" format="enum">
        <enum name="invisible" value="1"/>
        <enum name="visible" value="0"/>
    </attr>
    <attr name="titleLayout" format="reference"/>
    <!--Start styleable definition block-->
    <declare-styleable name="DemoListRowView">
        <attr name="android:background"/>
        <attr name="android:gravity"/>
        <attr name="android:text"/>
        <attr name="selectionHandler"/>
        <attr name="selectionHandlerVisiblity"/>
    </declare-styleable>
    <declare-styleable name="WidgetContainer">
        <attr name="android:layout"/>
        <attr name="android:text"/>
        <attr name="titleLayout"/>
    </declare-styleable>
    <!--End of any blocks-->
    
    
    <declare-styleable name="ViewFlow">
        <attr name="sidebuffer" format="integer" />
    </declare-styleable>
    <declare-styleable name="CircleFlowIndicator">
        <attr name="activeColor" format="color" />
        <attr name="inactiveColor" format="color" />
        <attr name="radius" format="dimension" />
		<attr name="centered" format="boolean" />
		<attr name="fadeOut" format="integer" />
		<attr name="inactiveType">
			<flag name="stroke" value="0" />
			<flag name="fill" value="1" />
		</attr>
		<attr name="activeType">
			<flag name="stroke" value="0" />
			<flag name="fill" value="1" />
		</attr>
    </declare-styleable>    
    <declare-styleable name="TitleFlowIndicator">
        <attr name="titlePadding" format="dimension" />
        <!-- Left/right padding of not active view titles. -->
        <attr name="clipPadding" format="dimension" />
        <attr name="selectedColor" format="color" />
        <attr name="selectedBold" format="boolean" />
        <attr name="selectedSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="footerLineHeight" format="dimension" />
        <attr name="footerColor" format="color" />
        <attr name="footerTriangleHeight" format="dimension" />
        <attr name="customTypeface" format="string" />
    </declare-styleable>
</resources>
