<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Holo.Demo" />

    <style name="Holo.Demo.FontViewer">
        <item name="android:gravity">center</item>
        <item name="android:layout_height">?listPreferredItemHeightSmall</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>

    <style name="Holo.Demo.ListRowView">
        <item name="android:background">?selectableItemBackground</item>
    </style>

    <style name="Holo.Demo.Theme" parent="Holo.Theme">

        <!-- Include block: DemoThemeDark -->
        <!-- Include block: DemoTheme -->
        <item name="android:homeAsUpIndicator">?homeAsUpIndicator</item>
        <item name="demoListRowViewStyle">@style/Holo.Demo.ListRowView</item>
        <item name="homeAsUpIndicator">@drawable/home_as_up_indicator</item>
        <!-- End of block DemoTheme -->
        <!-- End of block DemoThemeDark -->
        <!-- Include block: DemoThemeBarDark -->
        <!-- Include block: DemoThemeBar -->
        <!-- End of block DemoThemeBar -->
        <!-- End of block DemoThemeBarDark -->
        <item name="ic_menu_more_draw">@drawable/abc_ic_menu_moreoverflow_normal_holo_dark</item>
        <item name="detail_bottom_background">@color/background_holo_dark</item>
    </style>

    <style name="Holo.Demo.Theme.Fullscreen">

        <!-- Include block: Fullscreen -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="windowContentOverlay">@null</item>
        <!-- End of block Fullscreen -->

    </style>

    <style name="Holo.Demo.Theme.Light" parent="Holo.Theme.Light">

        <!-- Include block: DemoThemeLight -->
        <!-- Include block: DemoTheme -->
        <item name="android:homeAsUpIndicator">?homeAsUpIndicator</item>
        <item name="demoListRowViewStyle">@style/Holo.Demo.ListRowView</item>
        <item name="homeAsUpIndicator">@drawable/home_as_up_indicator</item>
        <!-- End of block DemoTheme -->
        <!-- End of block DemoThemeLight -->
        <!-- Include block: DemoThemeBarLight -->
        <!-- Include block: DemoThemeBar -->
        <!-- End of block DemoThemeBar -->
        <!-- End of block DemoThemeBarLight -->
        <item name="ic_menu_more_draw">@drawable/abc_ic_menu_moreoverflow_normal_holo_light</item>
        <item name="detail_bottom_background">@color/background_holo_light</item>
    </style>

    <style name="Holo.Demo.Theme.Light.DarkActionBar" parent="Holo.Theme.Light.DarkActionBar">

        <!-- Include block: DemoThemeLight -->
        <!-- Include block: DemoTheme -->
        <item name="android:homeAsUpIndicator">?homeAsUpIndicator</item>
        <item name="demoListRowViewStyle">@style/Holo.Demo.ListRowView</item>
        <item name="homeAsUpIndicator">@drawable/home_as_up_indicator</item>
        <!-- End of block DemoTheme -->
        <!-- End of block DemoThemeLight -->
        <!-- Include block: DemoThemeBarDark -->
        <!-- Include block: DemoThemeBar -->
        <!-- End of block DemoThemeBar -->
        <!-- End of block DemoThemeBarDark -->
        <item name="ic_menu_more_draw">@drawable/abc_ic_menu_moreoverflow_normal_holo_dark</item>
        <item name="detail_bottom_background">@color/background_holo_dark</item>
    </style>

    <style name="Holo.Demo.Theme.Light.DarkActionBar.Fullscreen">

        <!-- Include block: Fullscreen -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="windowContentOverlay">@null</item>
        <!-- End of block Fullscreen -->

    </style>

    <style name="Holo.Demo.Theme.Light.Fullscreen">

        <!-- Include block: Fullscreen -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="windowContentOverlay">@null</item>
        <!-- End of block Fullscreen -->

    </style>

</resources>